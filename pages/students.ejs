<h1>Students</h1>
<table border="1">
  <thead>
    <tr>
      <th>Student ID</th>
      <th>Name</th>
      <th>Age</th>
    </tr>
  </thead>
  <tbody>
    <% students.forEach(student => { %> 
    <tr>
      <td><%= student.sid %></td>
      <td><%= student.name %></td>
      <td><%= student.age %></td>
    </tr>
    <% }); %>
  </tbody>
</table>
